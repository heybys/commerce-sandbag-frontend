    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/node": "24.0.1",
    "@types/react": "19.1.8",
    "commander": "^14.0.0",
    "dotenv-flow": "^4.1.0",
    "eslint": "^9.29.0",
    "eslint-config-next": "15.3.3",
    "prettier": "^3.5.3",
    "typescript": "5.8.3"
  }
